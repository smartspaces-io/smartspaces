<?xml version="1.0"?>
<project type="activity" builder="java">
  <name>Gesture Example</name>
  <description>
    An activity that uses a connected gesture detection device to register gestural
    events.
  </description>

  <identifyingName>
    smartspaces.example.activity.image.gesture
  </identifyingName>
  <version>1.0.0</version>

  <activity type="smartspaces_native">
    <name>smartspacesExampleActivityGesture</name>
    <class>
      io.smartspaces.example.activity.image.gesture.GestureExampleActivity
    </class>

    <configuration>
      <property name="space.activity.log.level" value="info" />
    </configuration>
  </activity>
</project>
